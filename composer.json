{
    "name": "spatie/laravel-tags",
    "description": "Add tags and taggable behaviour to your Laravel app",
    "keywords": [
        "spatie",
        "laravel-tags"
    ],
    "homepage": "https://github.com/spatie/laravel-tags",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/container": "~5.3.0|~5.4.0",
        "illuminate/contracts": "~5.3.0|~5.4.0",
        "illuminate/database": "~5.3.0|~5.4.0",
        "illuminate/support": "~5.3.0|~5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "orchestra/testbench": "~3.3.0|~3.4.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Tags\\": "src"
        },
        "classmap": ["vendor/spatie/eloquent-sortable", "vendor/spatie/laravel-translatable"]

    },

    "autoload-dev": {
        "psr-4": {
            "Spatie\\Tags\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
  }
}
